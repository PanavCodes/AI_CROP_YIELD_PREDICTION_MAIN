# External API Keys Configuration
# Get these keys from the respective service providers

# OpenWeatherMap API Key (Primary weather data source)
# Get free API key from: https://openweathermap.org/api
VITE_OPENWEATHER_API_KEY=your_openweather_api_key_here

# Alpha Vantage API Key (for financial/commodity data)
# Get free API key from: https://www.alphavantage.co/support/#api-key
VITE_ALPHAVANTAGE_API_KEY=your_alphavantage_api_key_here

# Backend API URL
VITE_API_BASE_URL=http://localhost:5000

# Legacy Weather API Configuration (fallback)
REACT_APP_WEATHER_API_KEY=your_openweathermap_api_key_here
REACT_APP_WEATHER_PROVIDER=openweathermap
REACT_APP_WEATHER_CACHE_DURATION=12

# Location Services
REACT_APP_ENABLE_GPS_LOCATION=true
REACT_APP_DEFAULT_COUNTRY=IN

# Development Settings
REACT_APP_DEBUG_WEATHER=false

# Google Maps API Configuration
# Get your API key from https://console.cloud.google.com/apis/credentials
VITE_GOOGLE_MAPS_API_KEY=AIzaSyAfFDVXypkWCuA-nntCrAgvmuIjMGPe8dM
